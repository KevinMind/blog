{
  "name": "app-blog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "codegen": "graphql-let",
    "build": "yarn codegen && next build",
    "start": "next start ",
    "dev": "yarn codegen && next dev",
    "lint": "TIMING=1 next lint",
    "format": "yarn lint --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next",
    "test-storybook": "test-storybook --url http://localhost:7006",
    "test": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"yarn build-storybook --quiet && npx http-server storybook-static --port 7006 --silent\" \"wait-on tcp:7006 && yarn test-storybook\"",
    "storybook": "start-storybook -p 7006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "graphql": "^15.7.2",
    "logger": "*",
    "next": "^12.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ui": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/import-types-preset": "^2.1.15",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@graphql-codegen/typescript-react-apollo": "^3.2.11",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/test-runner": "^0.0.4",
    "@storybook/testing-library": "^0.0.9",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^17.0.12",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "babel-loader": "^8.2.3",
    "eslint": "^7.32.0",
    "eslint-plugin-storybook": "^0.5.7",
    "graphql-let": "^0.18.6",
    "msw": "^0.39.2",
    "msw-storybook-addon": "^1.6.1",
    "scripts": "*",
    "tsconfig": "*",
    "typescript": "^4.5.3"
  }
}
